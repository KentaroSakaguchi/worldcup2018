    "dist": "npm run stylelint && npm run cleandir && npm run copylib && npm run html && npm run imagemin && npm run js && npm run styles",
    "build": "npm run stylelint:build && npm run copylib:build && npm run html:build && npm run imagemin:build && npm run js:build && npm run styles:build"
